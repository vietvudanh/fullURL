{
    "manifest_version": 2,
    "name": "fullURL",
    "version": "1.0",
    "description": "Simple extension for enter URL in Chrome's fullscreen mode",
    "author": "Viet Vu",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "third-party/jquery-1.11.2.min.js",
                "js/eventPage.js"
            ],
            "css": [
                "css/main.css"
            ]
        }
    ],
    "permissions": [
        "activeTab",
        "tabs"
    ]
}